@model IEnumerable<NotesMarketPlace.Models.memberDetails>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>

    <!-- Important meta tags -->
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <!--Title-->
    <title> Notes - MarketPlace</title>

    <!--Google Fonts-->

    <link href="https://fonts.googleapis.com/css2?family=Open+Sans
             :wght@300;400;600;700&display=swap" rel="stylesheet">

    <!--Bootstrap CSS-->
    <link rel="stylesheet" href="~/css/bootstrap.min.css">

    <!--Fontawesome-->
    <link rel="stylesheet" href="~/font-awesome/css/font-awesome.css">

    <!--Custom CSS-->
    <link rel="stylesheet" href="~/cssA/admin-memberspage.css">

    <!--Responsive CSS-->
    <link rel="stylesheet" href="~/cssA/admin-responsive-manageadmin.css">

</head>

<body>

    <!--Header-->
    <header>
        <nav class="navbar navbar-fixed-top">
            <div class="container">
                <div class="site-nav-wrapper">

                    <div class="navbar-header">

                        <!--Mobile menu Open Button-->
                        <span id="mobile-nav-open-btn">&#9776;</span>

                        <!--Logo-->
                        <a href="#home" class="navbar-brand"><img src="~/home/logo.png"></a>
                    </div>

                    <div class="container">
                        <div class="collapse navbar-collapse">
                            <ul class="nav navbar-nav pull-right ">
                                <li><a href="#search" class="active">Dashboard</a></li>
                                <li><a href="#sell">Notes</a></li>
                                <li><a href="#FAQ">Members</a></li>
                                <li><a href="#contact">Reports</a></li>
                                <li><a href="#contact">Settings</a></li>
                                <li><a href="#profile" class="photo"><img src="~/img/Customers/client-1.jpg" class="img-circle"></a></li>
                                <li><a href="#login">Logout</a></li>
                            </ul>
                        </div>
                    </div>


                    <!--Mobile Menu-->
                    <div id="mobile-nav">
                        <!--Mobile Menu Close Button-->
                        <span id="mobile-nav-close-btn">&times;</span>
                        <div id="mobile-nav-content">
                            <ul class="nav">
                                <li><a href="#profile" class="photo"><img src="~/img/Customers/client-1.jpg" class="img-circle"></a></li>
                                <li><a href="#search">Dashboard</a></li>
                                <li><a href="#sell">Notes</a></li>
                                <li><a href="#FAQ">Members</a></li>
                                <li><a href="#contact">Reports</a></li>
                                <li><a href="#contact">Settings</a></li>
                                <li><a href="#login">Logout</a></li>
                            </ul>
                        </div>
                    </div>

                </div>
            </div>
        </nav>
    </header>
    <!--Header ends-->
    <!-- Member Detail -->

    <section id="details">

        <div class="container">

            <div class="row">
                <!-- Left Side -->

                @* This is for Me,ber's Details*@
                @{ NotesMarketPlace.Models.User u1 = ViewBag.D1;
                    NotesMarketPlace.Models.UserProfile u2 = ViewBag.D2;
                 }

                <div class="col-md-6">

                    <div class="row">
                        <div class="col-md-6">
                            <img src="~/img/Admin_images/images/member.png">
                        </div>
                        <div class="col-md-6 dev">
                            <div class="row sp">First Name:<span>@u1.First_Name</span></div>
                            <div class="row sp">Last Name:<span>@u1.Last_Name</span></div>
                            <div class="row sp">Email:<span>@u1.EmailID</span></div>
                            <div class="row sp">DOB:<span>@u2.DOB</span></div>
                            <div class="row sp">Phone Number:<span>@u2.Phone_Number</span></div>
                            <div class="row sp">College/University:<span>@u2.College</span></div>
                        </div>

                    </div>
                </div>

                <div class="col-md-6 dev">

                    <div class="col-md-1 v1">  </div>
                    <div class="col-md-11">
                        <div class="row sp">Address 1:<span>@u2.Address_Line1</span></div>
                        <div class="row sp">Address 2:<span>@u2.Address_Line2</span></div>
                        <div class="row sp">City:<span>@u2.City</span></div>
                        <div class="row sp">State:<span>@u2.State</span></div>
                        <div class="row sp">Country:<span>@u2.Country</span></div>
                        <div class="row sp">
                            Zip Code:<span>@u2.ZipCode</span>

                        </div>

                    </div>

                </div>

            </div>

        </div>

    </section>

    <hr class="container">
    <!--Table-->
    <section id="downloads">
        <div class="container">
            <div class="row search">
                <div class="col-md-6">
                    <h4>Members</h4>
                </div>
                <div class="col-md-6 text-right">
                    <form class="form-inline">
                        <div class="form-group mx-sm-3 mb-2 bl-767">

                            <input type="text" class="form-control" id="search" placeholder="Search">
                        </div>
                        <button type="submit" class="btn btn-primary mb-2 btn-bl">Search</button>
                    </form>
                </div>
            </div>
            <table class="table" id="tableData">
                <thead>
                    <tr>
                        <th scope="col">SR NO.</th>
                        <th scope="col">NOTE TITLE</th>
                        <th scope="col">CATEGORY</th>
                        <th scope="col">STATUS</th>
                        <th scope="col">DOWNLOADED NOTES</th>
                        <th scope="col">TOTAL EARNINGS</th>
                        <th scope="col">DATE ADDED</th>
                        <th scope="col">PUBLISHED DATE</th>
                        <th scope="col"> </th>
                    </tr>
                </thead>
                <tbody>
                    @* This is table MemberDetails *@
                    @{ int x = 1;}

                    @foreach (var item in Model)
                    {
                        <tr>
                            <td><span>@(x++)</span></td>
                            <td>@item.snote.Title</td>
                            <td>@item.snote.Category</td>
                            <td>@item.snote.Status</td>
                            <td>@item.DownloadedNotes</td>
                            <td><span>$15</span></td>
                            <td><span>09-10-2020, 10:10</span></td>
                            <td><span>09-10-2020, 10:10</span></td>
                            <td class="dropdown">
                                <img src="~/img/images/dots.png" class="dropbtn" onclick="myFunction()">
                                <div id="myDropdown" class="dropdown-content"><a href="#">Download Notes</a></div>
                            </td>
                        </tr>
                    }

                </tbody>
            </table>

            <nav aria-label="Page navigation example" class="text-center">
                <ul class="pagination ">
                    <li class="page-item prev">
                        <a class="page-link" href="#" aria-label="Previous">
                            <span aria-hidden="true">&lt;</span>
                            <span class="sr-only">Previous</span>
                        </a>
                    </li>
                    <li class="page-item"><a class="page-link active" href="#">1</a></li>
                    <li class="page-item"><a class="page-link" href="#">2</a></li>
                    <li class="page-item"><a class="page-link" href="#">3</a></li>
                    <li class="page-item"><a class="page-link" href="#">4</a></li>
                    <li class="page-item"><a class="page-link" href="#">5</a></li>
                    <li class="page-item prev">
                        <a class="page-link" href="#" aria-label="Next">
                            <span aria-hidden="true">&gt;</span>
                            <span class="sr-only">Next</span>
                        </a>
                    </li>
                </ul>
            </nav>

        </div>
    </section>
    <hr>
    <footer>
        <div class="container">
            <div class="row">
                <div class="col-md-6">
                    <p>Copyright &#169; Tatvasoft All rights reserved.</p>
                </div>
                <div class="col-md-6 text-right">
                    <ul class="social-list">
                        <li><a href="#"><i class="fa fa-facebook"></i></a></li>
                        <li><a href="#"><i class="fa fa-twitter"></i></a></li>
                        <li><a href="#"><i class="fa fa-google-plus"></i></a></li>
                    </ul>
                </div>
            </div>
        </div>
    </footer>


    <!--JS-->
    <script src="~/js/bootstrap.min.js"></script>
    <script src="~/js/jquery.js"></script>
    <script src="~/Client-side-HTML-Table-Pagination-Plugin-with-jQuery-Paging/paging.js"></script>
    <script src="~/js/admin-memberspage.js"></script>
</body>

</html>
